{
    "name": "uuf6429/electron-installer",
    "description": "A Composer package which installs the Electron binary (Linux, Windows, Mac) into `/bin` of your project.",
    "type": "custom-installer",
    "keywords": ["electron", "headless", "binaries"],
    "license": "MIT",
    "authors": [
        {
            "name": "Christian Sciberras",
            "email": "christian@sciberras.me"
        },
        {
            "name": "Jens-Andr√© Koch",
            "email": "jakoch@web.de"
        }
    ],
    "require": {
        "php": "^7",
        "ext-bz2": "*",
        "ext-openssl": "*"
    },
    "require-dev": {
        "composer/composer": "^2",
        "phpunit/phpunit": "^8"
    },
    "autoload": {
        "psr-4": {
            "ElectronInstaller\\Test\\" : "tests/"
        },
        "psr-0": {
            "ElectronInstaller\\": "src/"
        }
    }
}
